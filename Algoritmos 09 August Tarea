#include <iostream>
#include <conio.h>

using namespace std;
void main ()
{
	int a, b;
	cout<<"Ingrese dos numeros"<<endl;
	cin>>a;
	cin>>b;
	if (a>b)
		{
		cout<< "El mayor de los dos numeros es "<< a<<endl;
		}
	else if (b>a)
		{
		cout<< "El mayor de los dos numeros es "<< b<<endl;
		}
	else
		{
		cout<<"Son Iguales"<<endl;
		}
	getch();
}
